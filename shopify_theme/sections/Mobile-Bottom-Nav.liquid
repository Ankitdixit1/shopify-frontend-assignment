  "settings": [
    {
      "type": "header",
      "content": "Important Note"
    },
    {
      "type": "paragraph",
      "content": "This navigation bar is designed to be fixed at the bottom of the screen and is hidden on desktop devices (screens wider than 768px). To make it appear, view your store on a mobile device or resize your browser window."
    }
  ],
  "blocks": [
    {
      "type": "nav_item",
      "name": "Navigation Item",
      "settings": [
        {
          "type": "text",
          "id": "text",
          "label": "Link Text",
          "default": "Home"
        },
        {
